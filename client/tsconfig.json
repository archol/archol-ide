{
  "compilerOptions": {
    "incremental": true, 
    "target": "es5",     
    "jsx": "react",    
    "module": "commonjs",
    "lib": [
      "ES2020",
      "DOM"
    ],                           
    "outDir": "./.tmp/dist",              
    "tsBuildInfoFile": "./.tmp/buildinfo",
    "noEmit": true,
    "importHelpers": true,
    "strict": true,
    "esModuleInterop": true,  
    "forceConsistentCasingInFileNames": true
  }
}
