{
  "compilerOptions": {
    "incremental": true,
    "target": "ES5",
    "module": "commonjs",
    "lib": [
      "ES2017"
    ],
    "sourceMap": true,
    "outDir": ".temp/dist",
    "tsBuildInfoFile": ".temp/cache",
    "rootDir": "./src",
    "baseUrl": "./src",
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*.ts", "src/generate/generator.ts.txt"
  ],
  "exclude": [
  ]
}